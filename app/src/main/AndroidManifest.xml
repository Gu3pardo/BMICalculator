<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="guepardoapps.bmicalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="guepardoapps.bmicalculator.views.AboutView" />
        <activity
            android:name="guepardoapps.bmicalculator.views.BootView"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="guepardoapps.bmicalculator.views.GraphView" />
        <activity android:name="guepardoapps.bmicalculator.views.InputView" />
    </application>

</manifest>
